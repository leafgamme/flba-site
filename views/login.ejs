            background-color: #FFB3C8;
            border: none;
            border-radius: 5px;
            color: #000;
            cursor: pointer;
            display: block;
            font-family: 'Roboto', monospace;
            font-size: 16px;
            margin: auto;
            padding: 15px 0;
            width: 80%;
        }

        .login-form h2 {
            text-align: center;
            font-family: 'Roboto', monospace;
            font-size: 14px;
            font-weight: 100;
            margin-top: 20px;
            color: red;
        }

        .login-form h2.success {
            color: green;
        }

        h1 {
            text-align: center;
            font-family: 'Roboto', monospace;
            font-size: 30px;
            margin-top: 20px;
        }

        @media screen and (max-width: 768px) {
            .login-form {
                margin-left: 5%;
                margin-right: 5%;
                max-width: none;
                width: 80%;
                height: 200px;
            }

            .login-form input[type="text"],
            .login-form input[type="password"] {
                font-size: 14px;
                margin-bottom: 10px;
            }
